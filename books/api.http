# Get all books
GET http://192.168.80.3:3001/books HTTP/1.1
Content-Type: application/json


###

# Create a book
POST http://192.168.80.3:3001/books HTTP/1.1
Content-Type: application/json

{
    "title": "The Lord of the Rings"
}

###

# Update a book
PUT http://192.168.80.3:3001/books/1 HTTP/1.1
Content-Type: application/json

{
    "title": "The Lord of the Rings 2"
}

###

# Delete a book
DELETE http://192.168.80.3:3001/books/1 HTTP/1.1
Content-Type: application/json
